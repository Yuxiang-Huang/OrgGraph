{
  "name": "ScottyStack",
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "prettier": "^3.7.4",
    "syncpack": "^13.0.4",
    "turbo": "^2.7.4",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.3.5",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "check-types": "turbo run check-types",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
